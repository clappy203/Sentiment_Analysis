        "od.download(\"https://www.kaggle.com/datasets/lakshmi25npathi/imdb-dataset-of-50k-movie-reviews\")\n",
        "df = pd.read_csv(\"imdb-dataset-of-50k-movie-reviews/IMDB Dataset.csv\")\n"
