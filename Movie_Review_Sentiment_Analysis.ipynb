    {
      "cell_type": "code",
      "source": [
        "# ===============================\n",
        "# 3. Text Preprocessing\n",
        "# ===============================\n",
        "def clean_review(text: str) -> str:\n",
        "    \"\"\"\n",
        "    Clean a movie review by removing HTML tags, special characters, and extra whitespace.\n",
        "\n",
        "    Args:\n",
        "        text (str): The raw text of the movie review.\n",
        "\n",
        "    Returns:\n",
        "        str: The cleaned review text.\n",
        "\n",
        "    Notes:\n",
        "        - HTML tags like <br /> are removed.\n",
        "        - Only alphanumeric characters and spaces are kept.\n",
        "        - Consecutive whitespace characters are reduced to a single space.\n",
        "        - Leading and trailing whitespace is stripped.\n",
        "    \"\"\"\n",
        "    text = re.sub(r\"<.*?>\", \" \", text)       # Remove HTML tags\n",
        "    text = re.sub(r\"[^a-zA-Z0-9\\s]\", \"\", text)  # Keep only alphanumerics\n",
        "    text = re.sub(r\"\\s+\", \" \", text).strip()    # Normalize spaces\n",
        "    return text\n",
        "\n",
        "df[\"review_clean\"] = df[\"review\"].apply(clean_review)\n"
      ],
      "metadata": {
        "id": "LfCKkru8eG4M"
      },
      "execution_count": 8,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "# ===============================\n",
        "# 4. Tokenization\n",
        "# ===============================\n",
        "def prepare_inputs(tokenizer: BertTokenizer, texts: List[str], max_len: int = 128) -> dict:\n",
        "    \"\"\"\n",
        "      Tokenize a list of texts and convert them into BERT input features.\n",
        "\n",
        "      Args:\n",
        "          tokenizer (BertTokenizer): Pretrained BERT tokenizer.\n",
        "          texts (List[str]): List of text strings to tokenize.\n",
        "          max_len (int, optional): Maximum sequence length for padding/truncation.\n",
        "              Defaults to 128.\n",
        "\n",
        "      Returns:\n",
        "          dict: Dictionary containing:\n",
        "              - input_ids (torch.Tensor): Token IDs for the input texts.\n",
        "              - attention_mask (torch.Tensor): Attention masks corresponding to input_ids.\n",
        "\n",
        "      Notes:\n",
        "          - Pads sequences to max_len and truncates longer sequences.\n",
        "          - Returns PyTorch tensors ready for model input.\n",
        "    \"\"\"\n",
        "    inputs = tokenizer(\n",
        "        texts,\n",
        "        truncation=True,\n",
        "        padding=\"max_length\",\n",
        "        max_length=max_len,\n",
        "        return_attention_mask=True,\n",
        "        return_tensors=\"pt\"\n",
        "    )\n",
        "    return {\"input_ids\": inputs[\"input_ids\"], \"attention_mask\": inputs[\"attention_mask\"]}\n",
        "\n",
        "# Load pretrained BERT tokenizer\n",
        "tokenizer = BertTokenizer.from_pretrained(\"bert-base-uncased\")\n",
        "\n",
        "# Tokenize dataset\n",
        "features = prepare_inputs(tokenizer, df[\"review_clean\"].tolist())\n",
        "input_ids = features[\"input_ids\"]\n",
        "attention_mask = features[\"attention_mask\"]\n",
        "labels = torch.tensor(df[\"sentiment\"].apply(lambda x: 1 if x == \"positive\" else 0).values)\n",
        "\n",
        "# Wrap into TensorDataset\n",
        "dataset = TensorDataset(input_ids, attention_mask, labels)"
      ],
